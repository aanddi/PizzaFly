{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "trailingComma": "none",
  "arrowParens": "avoid",
  "singleQuote": true,
  "printWidth": 150,
  "useTabs": false,
  "tabWidth": 2,
  "semi": false,
  "bracketSameLine": false,
  "htmlWhitespaceSensitivity": "strict",
  "proseWrap": "always",
  "jsxBracketSameLine": true,
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/api/(.*)$",
    "^@/assets/(.*)$",
    "^@/components/(.*)$",
    "^@/services/(.*)$",
    "^@/types/(.*)$",
    "^../(.*)",
    "^./(.*)"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
